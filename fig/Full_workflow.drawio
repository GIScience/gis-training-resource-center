        <mxCell id="cOcfP2GvxwVNDIPguZ_v-33" value="QGIS Tool:&amp;nbsp;Join attribute by location (summary)" style="rounded=0;whiteSpace=wrap;html=1;align=left;fontSize=14;fillColor=#f8cecc;strokeColor=#b85450;opacity=50;movable=1;resizable=1;rotatable=1;deletable=1;editable=1;locked=0;connectable=1;verticalAlign=top;spacingLeft=5;spacingTop=5;" parent="1" vertex="1">
